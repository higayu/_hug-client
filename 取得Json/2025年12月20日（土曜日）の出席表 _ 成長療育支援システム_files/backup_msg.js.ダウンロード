$(function() {

    if (checkBackFile()) {
        $('.backupMsg').fadeIn();
    }
});


function checkBackFile(){

    var url = prePath + "./ajax/ajax_backfiles.php";

    var result = 0;

    $.ajax({
        url: url,
        timeout: 3000,
        async:false,
        dataType: 'json',
        success: function (data) {
            if (data == 1) {
                result = 1;
            }else{
                result = 0;
            }
        }
    });

    return result;
}